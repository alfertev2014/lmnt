{
  "compilerOptions": {
    "outDir": "./lib",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noPropertyAccessFromIndexSignature": true,
    "module": "Node16",
    "target": "ES2020",
    "declaration": true,
    "isolatedModules": true,
    "esModuleInterop": true
  }
}