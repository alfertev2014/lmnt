{
  "name": "rwrtw",
  "version": "0.0.9",
  "description": "Tiny UI library for browser applications, thin DOM helpers and component building blocks with transparent and convenient DSL",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "README.md"
  ],
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./lib/index.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "prepack": "npm run clean && npm run prettier && npm run lint && npm run build",
    "build": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "clean": "rm -rf lib"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alfertev2014/rwrtw.git"
  },
  "keywords": [
    "tiny",
    "small",
    "DOM",
    "helpers",
    "UI",
    "library",
    "components",
    "typescript"
  ],
  "author": "Vasiliy Alfertev <alfertev2012@gmail.com> (https://github.com/alfertev2014)",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jest": "^29.5.12",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "rollup": "^4.21.1",
    "rollup-plugin-ts": "^3.4.5",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  }
}
